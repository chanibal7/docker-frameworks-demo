<?xml version="1.0" encoding="UTF-8"?>  
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee" id="WebApp_ID" version="2.5">  
  <display-name>RESTfulWebServiceExample</display-name>  
  <servlet>  
    <servlet-name>postgressql</servlet-name>  
    <servlet-class>com.postgressql.client.PostgresSQLClient</servlet-class>  
    <load-on-startup>1</load-on-startup>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>postgressql</servlet-name>  
    <url-pattern>/client/*</url-pattern>  
  </servlet-mapping>  
</web-app>   
